{
  "name": "@heraclius/external-deps-plugin",
  "version": "0.1.1",
  "description": "Vite插件。它的主要功能是从 Vite 的打包过程中排除 `package.json` 文件中列出的 `dependencies` 和 `peerDependencies` 依赖",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "vite build",
    "watch": "vite build --watch",
    "test": "vitest run"
  },
  "devDependencies": {
    "@types/node": "latest",
    "unplugin-swc": "latest",
    "vite": "latest",
    "vite-plugin-dts": "latest",
    "vitest": "latest"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "vite": "latest"
  }
}
